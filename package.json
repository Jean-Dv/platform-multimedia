{
  "name": "platform-multimedia",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev:auth:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/apps/auth/backend/start.ts",
    "dev:multimedia:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/apps/multimedia/backend/start.ts",
    "dev:backoffice_multimedia:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/apps/backoffice-multimedia/backend/start.ts",
    "lint": "eslint ./src/**/*.ts ./tests/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts ./tests/**/*.ts --fix",
    "prettier": "prettier --check ./src/**/*.ts ./tests/**/*.ts",
    "prettier:fix": "prettier --write ./src/**/*.ts ./tests/**/*.ts",
    "build": "pnpm run build:clean && pnpm run build:tsc && pnpm run build:di",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "copy 'src/**/*.{json,yaml,html,png}' dist/src",
    "build:clean": "rm -r dist; exit 0",
    "test": "pnpm run test:unit && pnpm run test:features",
    "test:unit": "NODE_ENV=test jest",
    "test:features": "pnpm run test:auth:backend:features && pnpm run test:multimedia:backend:features && pnpm run test:backoffice_multimedia:backend:features && pnpm run test:backoffice_subscriptions:backend:features",
    "test:auth:backend:features": "NODE_ENV=test cucumber-js -p authBackend --exit",
    "test:multimedia:backend:features": "NODE_ENV=test cucumber-js -p multimediaBackend --exit",
    "test:backoffice_multimedia:backend:features": "NODE_ENV=test cucumber-js -p backofficeMultimediaBackend --exit",
    "test:backoffice_subscriptions:backend:features": "NODE_ENV=test cucumber-js -p backofficeSubscriptionsBackend --exit",
    "command:auth:rabbitmq": "NODE_ENV=production ts-node src/apps/auth/backend/command/runConfigureRabbitMQCommand",
    "command:multimedia:rabbitmq": "NODE_ENV=production ts-node src/apps/multimedia/backend/command/runConfigureRabbitMQCommand",
    "command:backoffice_multimedia:rabbitmq": "NODE_ENV=production ts-node src/apps/backoffice-multimedia/backend/command/runConfigureRabbitMQCommand",
    "command:backoffice_subscriptions:rabbitmq": "NODE_ENV=production ts-node src/apps/backoffice-subscriptions/backend/command/runConfigureRabbitMQCommand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "3.490.0",
    "@aws-sdk/s3-request-presigner": "3.490.0",
    "amqplib": "0.10.3",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "convict": "6.2.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "express-validator": "7.0.1",
    "glob": "10.3.10",
    "helmet": "7.1.0",
    "http-proxy-middleware": "2.0.6",
    "http-status": "1.7.3",
    "jsonwebtoken": "9.0.2",
    "module-alias": "2.2.3",
    "mongodb": "6.3.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-dependency-injection": "2.6.11",
    "redis": "4.6.10",
    "uuid": "9.0.1",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@cucumber/cucumber": "10.0.1",
    "@faker-js/faker": "8.3.1",
    "@types/amqplib": "0.10.4",
    "@types/bcrypt": "5.0.2",
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/convict": "6.1.6",
    "@types/cors": "2.8.16",
    "@types/errorhandler": "1.5.3",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.8",
    "@types/jsonwebtoken": "9.0.5",
    "@types/module-alias": "2.0.4",
    "@types/morgan": "1.9.9",
    "@types/multer": "1.4.11",
    "@types/supertest": "2.0.16",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "6.11.0",
    "copy": "0.3.2",
    "eslint": "8.0.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "40.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0 || 16.0.0 ",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.0.0",
    "husky": "8.0.0",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "pnpm run lint:fix",
      "pnpm run prettier:fix"
    ]
  },
  "_moduleAliases": {
    "@Shared": "src/Contexts/Shared",
    "@Apps": "src/apps",
    "@Auth": "src/Contexts/Auth",
    "@Multimedia": "src/Contexts/Multimedia",
    "@BackofficeMultimedia": "src/Contexts/BackofficeMultimedia",
    "@BackofficeSubscriptions": "src/Contexts/BackofficeSubscriptions",
    "@Subscriptions": "src/Contexts/Subscriptions"
  }
}